<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/styles/custom_game/boss_list.css" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/custom_game/boss_list_item.js" />
	</scripts>
	<Panel class="BossListRow">
		<Image src="file://{images}/custom_game/arthas.png" id="background_image" class="BackGroundImage" />
		<Panel id="bossname_and_difficulty" class="BossNameAndDifficulty" >
			<Label text="#BossName" class="BossNameLabel" id="boss_name_label" />
			<Panel class="Difficulty_Selector">
				<RadioButton id="button_normal" onactivate="OnSelectDifficultyNormal()">
					<Label id = "difficulty_normal" text="Difficulty_Normal" group="difficulty_selector" />
				</RadioButton>
				<RadioButton id="button_hero" onactivate="OnSelectDifficultyHero()">
					<Label id = "difficulty_normal" text="Difficulty_Hero" group="difficulty_selector" />
				</RadioButton>
			</Panel>
		</Panel>
	</Panel>
</root>